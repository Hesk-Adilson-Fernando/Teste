    <DataSet Name="Balancete">
      <Query>
        <DataSourceName>Facturac</DataSourceName>
        <CommandText>select * from (
--Custos
select 1 as ordem,pgc.conta, pgc.descricao, integrador=(case when pgc.integracao=1 then 1 else 0 end),  
(ISNULL(sum(deb),0)-ISNULL(sum(cre),0)) as saldo,pgcsa.ano,grupo='Custos'
from pgcsa_vw as pgcsa (nolock) inner join pgc (nolock) on  
pgcsa.conta=pgc.conta and pgcsa.ano=pgc.ano
where left(pgcsa.conta,2) in('59','62','63','65','69')
/*{p}*/  and pgcsa.ano=2017 group by pgc.conta,pgc.descricao,pgc.integracao,pgcsa.ano 
  UNION  --Totais Custos
 Select 2 as ordem, '0' as conta,descricao='TOTAL DOS CUSTOS',integrador=1,saldo=(
(isnull(sum(deb),0))-(isnull(sum(cre),0))),pgcsa.ano
 ,grupo='Custos' from pgcsa_vw as pgcsa where left(pgcsa.conta,2) in('59','62','63','65','69') and pgcsa.ano=2017 and integracao=0
 group by pgcsa.ano
  UNION 
  --Proveitos
select 3 as ordem,pgc.conta, pgc.descricao, integrador=(case when pgc.integracao=1 then 1 else 0 end),  
(ISNULL(sum(deb),0)-ISNULL(sum(cre),0)) as saldo,pgcsa.ano,grupo='Proveitos'
from pgcsa_vw as pgcsa (nolock) inner join pgc (nolock) on  
pgcsa.conta=pgc.conta and pgcsa.ano=pgc.ano
where left(pgcsa.conta,2) in('71','72') and pgcsa.ano=2017
/*{p}*/  group by pgc.conta,pgc.descricao,pgc.integracao,pgcsa.ano 
  UNION  --Totais Proveitos
 Select 4 as ordem, '0' as conta,descricao='TOTAL DOS PROVEITOS',integrador=1,saldo=(
(isnull(sum(deb),0))-(isnull(sum(cre),0))),pgcsa.ano
 ,grupo='Proveitos' from pgcsa_vw as pgcsa where left(pgcsa.conta,2) in('71','72') and pgcsa.ano=2017 and integracao=0
 group by pgcsa.ano
   UNION 
  --Proveitos
select 3 as ordem,pgc.conta, pgc.descricao, integrador=(case when pgc.integracao=1 then 1 else 0 end),  
(ISNULL(sum(deb),0)-ISNULL(sum(cre),0)) as saldo,pgcsa.ano,grupo='Resultados'
from pgcsa_vw as pgcsa (nolock) inner join pgc (nolock) on  
pgcsa.conta=pgc.conta and pgcsa.ano=pgc.ano
where left(pgcsa.conta,2) in('821','831','85','881') and pgcsa.ano=2017
/*{p}*/  group by pgc.conta,pgc.descricao,pgc.integracao,pgcsa.ano 
 
 )tmp1 order by ordem,conta</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
