  <DataSets>
    <DataSet Name="ExtractoPgc">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select conta,descricao,descritivo,deb,cre, sum(deb-cre) over(PARTITION BY conta  order by ordem,data rows unbounded preceding) as saldo,
		            data,dinome,dilno,ordem from (
		            select isnull(conta,'') conta,isnull(descricao,'') descricao,isnull(descritivo,'Totais...') descritivo,isnull(sum(deb),0) deb,isnull(sum(cre),0) cre,isnull(data,'') data,isnull(dinome,'') dinome,
					isnull(dilno,0) dilno,ordem = isnull(ordem, 2) from(

		            select conta,descricao='',descritivo='Saldo anterior',sum(deb) deb,sum(cre) cre,data=0,dinome='',dilno=0,cast(-3 as numeric) as ordem,lcontstamp=''
		                from ml (nolock) where ml.conta = '111' 
		            and year(ml.data)=2017 and (ml.mes&lt;1 or (ml.mes=1 and DAY(ml.data)&lt;17)) group by conta 

		            union all 

		             select ml.conta,descricao,descritivo,deb,cre,ml.data,dinome,dilno,cast(-2 as numeric) as ordem,ml.lcontstamp
		            from ml (nolock) inner join lcont on ml.Lcontstamp=Lcont.Lcontstamp where ml.conta = '111'
                    and ml.data between '2017-01-17' and '2017-07-17' and ml.mes&gt;0
                    /*and ml.dia&gt;= 17 and ml.mes= 1 and year(ml.data)&gt;=2017*/

		            union all 
		            select conta='',descricao='',descritivo='',deb=0 ,cre=0,data=0,dinome='',dilno=0,cast(1 as numeric) as ordem,lcontstamp='')tmp0 
		            group by grouping sets((conta,dilno,descricao,descritivo,deb,cre, data,dinome,ordem), ())

		            )tmp1 order by ordem,YEAR(data),Month(data),DAY(data),dinome,dilno</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
